{
    "name": "robthree/urlgenerator",
    "type": "library",
    "description": "Friendly URL generator. Converts integers to words and back",
    "license": "MIT",
    "authors": [
        {
            "name": "RobIII",
            "homepage": "https://robiii.me"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "spatie/enum": "^3.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "phpstan/phpstan": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "RobThree\\UrlGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RobThree\\Tests\\UrlGenerator\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
